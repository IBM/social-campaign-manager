{
    "name": "social-campaign-manager",
    "description": "Social Media Campaigns for all with a click of a button.",
    "version": "1.1.0",
    "scripts": {
        "start": "npm run start:backend",
        "start:backend": "node --max_old_space_size=4096 app.js",
        "start:frontend": "npm run server:dev",
        "test": "npm run lint && npm run test:frontend && npm run test:backend",
        "lint": "npm run eslint && npm run tslint",
        "test:frontend": "karma start",
        "test:backend": "nyc --reporter=html mocha --timeout=3000  'spec/server/**/*spec.js'",
        "redis-browser": "open http://localhost:4567/ && redis-browser --config ./dev/redis-browser-config.yml",
        "build:dev": "npm run clean:dist && npm run webpack -- --config config/webpack.dev.js --progress --profile",
        "build:prod": "npm run clean:dist && npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
        "build": "npm run build:dev",
        "ci:travis:dev": "npm test && npm run build:dev",
        "ci:travis:prod": "npm run lint && npm run build:prod",
        "clean:dist": "npm run rimraf -- dist",
        "clean:install": "npm set progress=false && npm install",
        "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist compiled dll",
        "eslint": "eslint ./server",
        "generate-encryption-keys": "./dev/generate-encryption-keys.zsh",
        "github-deploy:dev": "npm run webpack -- --config config/webpack.github-deploy.js --progress --profile --env.githubDev",
        "github-deploy:test": "npm run webpack -- --config config/webpack.github-deploy.js --progress --profile --env.githubTest",
        "github-deploy:prod": "npm run webpack -- --config config/webpack.github-deploy.js --progress --profile --env.githubProd",
        "github-deploy": "npm run github-deploy:dev",
        "preclean:install": "npm run clean",
        "rimraf": "rimraf",
        "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
        "tslint": "tslint \"src/**/*.ts\"",
        "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
        "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
    },
